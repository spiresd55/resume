{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [ false ],
    "no-shadowed-variable": false,
    "no-var-requires": false,
    "no-console": false,
    "max-classes-per-file": false,
    "prefer-const": false,
    "one-variable-per-declaration": false,
    "radix": false,
    "no-unused-expression": false
  },
  "rulesDirectory": []
}